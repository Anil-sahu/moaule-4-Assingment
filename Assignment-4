<script type="text/javascript">
	var names =["johny", "Ravi", "Jarry"];
		var name=['jabJCDHIjKLMNjQRSJTUVWjXZ'];
	for(var i=0; i < name.length; i++){
        if (name[i]=='j') 
	   console.log("Goodbye!"+name[i]+" " +names[0]);
	else if (name[i]=='J') 
	 console.log("Goodbye!"+name[i]+" "+names[2])
			
	else
	 console.log("Hello!" +name[i]+" "+names[1]);	
}
			
</script>
